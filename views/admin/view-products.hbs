<section>
    <div class="container">
        <div class="row">
            <a href="/admin/add-products" class="btn btn-success ml-auto mt-3">Add product</a>
        </div>
        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col">No</th>
                    <th scope="col">Title</th>
                    <th scope="col">Category</th>
                    <th scope="col">Description</th>
                    <th scope="col">Image</th>
                </tr>
            </thead>
            <tbody>
                {{#each laptops}}
                <tr>
                    <th scope="row">1</th>
                    <td>{{this.name}}</td>
                    <td>Laptop</td>
                    <td>{{this.description}}</td>
                    <td><img style="height: 50px; width: 50px" src="{{this.image}}"></td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>